      "name": "Planning",
      "description": "",
      "stages": [
        {
          "name": "New Issues",
          "query": "issue.owner = \"unknown\" and issue.status = \"Open\" ",
          "description": "Unasigned issue",
          "targets": [
            "Assigned"
          ],
          "wipLimit": 6,
          "gates": null
        },
        {
          "name": "Assigned",
          "query": "issue.owner != \"unknown\" and issue.status = \"Open\"  ",
